  "scripts": {
    "watchold": "nodemon server.js && watchify -v -d -t [ babelify --presets [ es2015 react stage-2] ] main.js -o assets/compiled.js",
    "watch": "watchify -v -d -t [ babelify --presets [ es2015 react stage-2] ] main.js -o assets/compiled.js",
    "build": "NODE_ENV=production browserify -t [ reactify --es6 ] main.js | uglifyjs > assets/compiled.js",
